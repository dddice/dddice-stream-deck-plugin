{
  "name": "dddice-stream-deck-plugin",
  "version": "0.0.1",
  "description": "Roll 3D digital dice from your steam deck.",
  "repository": "git@github.com:dddice/dddice-stream-deck-plugin.git",
  "author": "dddice <developers@dddice.com>",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/code.ts",
    "src/settings.ts"
  ],
  "targets": {
    "default": {
      "outputFormat": "global",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "static"
      ],
      "extensions": "ts,html,css,png",
      "quiet": false
    }
  },
  "scripts": {
    "build": "parcel build --target default --dist-dir=com.dddice.roll.sdPlugin --public-url ./",
    "lint": "eslint src/*.ts*",
    "start": "npm-watch build"
  },
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "axios": "^0.27.2",
    "buffer": "^5.7.1",
    "crypto-browserify": "^3.12.0",
    "eslint-config-prettier": "^8.5.0",
    "events": "^3.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.1",
    "npm-watch": "^0.11.0",
    "parcel": "^2.7.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "prettier": "^2.8.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.1.8"
  },
  "keywords": [
    "dddice",
    "dice",
    "browser"
  ],
  "license": "ISC",
  "lint-staged": {
    "{**/*,*}.{css,json,html}": [
      "prettier --write"
    ],
    "*.ts*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "volta": {
    "node": "18.12.1",
    "npm": "9.1.2"
  },
  "staticFiles": {
    "staticPath": "static"
  }
}
